function _init()
	cartdata("tt_ld46")
	poke(0x5f5c, 255)
	
	for n=1,18 do
		add(grass,3+flr(rnd()*2))
	end
	
	for x=0,20 do
		for y=0,18 do
			if rnd()<0.85 then
				add(clouds,{16+flr(rnd()*6),x*7,y*-11})
			end
		end
	end
end

function _update60()
	f=f+1%30000
	cls(1)
	decide_screen()
end
