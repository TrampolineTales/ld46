function draw_world()
	for n=1,#clouds do
		spr(clouds[n][1],(clouds[n][2]+60-char_x)%136-7,((clouds[n][3]-char_y))+103)
	end

	for n=1,20 do
		spr(grass[n],((n*8+60-char_x)%136)-8,96+96-char_y)
	end
	
	rectfill(0,104+96-char_y,128,128,11)
end
